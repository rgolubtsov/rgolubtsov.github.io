* Copyright (C) 2016-2019 Radislav (Radicchio) Golubtsov
*/ %>

<% var    TITLE = sitename + sitename_; %>
<% var SUBTITLE = subtitle + subtitle_; %>
<% var DESCR    = subtitle;             %>
<% var CODER    = coder;                %>
<% var CSS      = css;                  %>

<!DOCTYPE html>
<html lang="en-US" dir="ltr">
<head>
<meta http-equiv="Content-Type"    content="text/html; charset=UTF-8" />
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
<meta       name="viewport"        content="width=device-width,initial-scale=1" />
<meta       name="description"     content="<%= DESCR %>" />
<meta       name="author"          content="<%= CODER %>" />
<meta       name="copyright"       content="Copyright (C) 2016-2019 <%= CODER %>" />
<title><%- TITLE %></title>
<link href="<%= CSS %>" type="text/css" rel="stylesheet" media="screen,projection" />
</head>
<body id="radicchio">
<table width="0" border="0" cellspacing="0" cellpadding="0" id="header">
<tbody>
<tr id="title">
<td rowspan="2" id="v-bar-h"></td>
<td id="title-td"><%- TITLE %></td>
</tr>
<tr id="subtitle">
<td id="subtitle-td"><%- SUBTITLE %></td>
</tr>
</tbody>
</table>
<%- yield %>
<table width="0" border="0" cellspacing="0" cellpadding="0" id="footer">
<tbody><tr><td id="v-bar-f"></td><td id="footer-td"></td></tr></tbody>
</table>
</body>
</html>
<%# vim:set nu et ts=4 sw=4: %>
